{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_AI_PROVIDER": "azure",
        "EXPO_PUBLIC_AI_API_URL": "https://azure-foundry-priytosh.services.ai.azure.com",
        "EXPO_PUBLIC_OPENAI_API_KEY": "@eas/azure-openai-api-key",
        "EXPO_PUBLIC_AZURE_DEPLOYMENT_NAME": "gpt-4.1",
        "EXPO_PUBLIC_AZURE_API_VERSION": "2024-10-21"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_AI_PROVIDER": "azure",
        "EXPO_PUBLIC_AI_API_URL": "https://azure-foundry-priytosh.services.ai.azure.com",
        "EXPO_PUBLIC_OPENAI_API_KEY": "@eas/azure-openai-api-key",
        "EXPO_PUBLIC_AZURE_DEPLOYMENT_NAME": "gpt-4.1",
        "EXPO_PUBLIC_AZURE_API_VERSION": "2024-10-21"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_AI_PROVIDER": "azure",
        "EXPO_PUBLIC_AI_API_URL": "https://azure-foundry-priytosh.services.ai.azure.com",
        "EXPO_PUBLIC_OPENAI_API_KEY": "@eas/azure-openai-api-key",
        "EXPO_PUBLIC_AZURE_DEPLOYMENT_NAME": "gpt-4.1",
        "EXPO_PUBLIC_AZURE_API_VERSION": "2024-10-21"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
